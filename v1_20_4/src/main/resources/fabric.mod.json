{
  "schemaVersion": 1,
  "id": "rptweaks",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "${authors}"
  ],
  "contact": {
    "repo": "${repo}"
  },
  "license": "MIT",
  "icon": "assets/rptweaks/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "dev.drtheo.rptweaks.RPTweaks"
    ],
    "modmenu": [
      "dev.drtheo.rptweaks.menu.RPTweaksModMenu"
    ],
    "client-early": [
      "dev.drtheo.rptweaks.RPTweaks"
    ]
  },
  "mixins": [
    "rptweaks.mixins.json"
  ],
  "accessWidener": "rptweaks.accesswidener",
  "depends": {
    "fabricloader": "*",
    "fabric-api": "*",
    "minecraft": ">=1.20.3"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "${discord}"
      }
    }
  }
}
